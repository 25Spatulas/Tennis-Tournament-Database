CREATE TABLE GAME ( 
TID     NUMBER(2)       NOT NULL, 
MATCHID NUMBER(3)       NOT NULL, 
SETNUM  NUMBER(1)       NOT NULL, 
GAMENUM NUMBER(1)       NOT NULL, 
P1SCORE NUMBER(1)       NOT NULL, 
P2SCORE NUMBER(1)       NOT NULL, 
CONSTRAINT PK_GAME PRIMARY KEY (TID, MATCHID, SETNUM, GAMENUM), 
FOREIGN KEY (TID, MATCHID, SETNUM) REFERENCES SSET(TID, MATCHID, SETNUM))
;

