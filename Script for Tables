CREATE TABLE MATCH ( 
TID         NUMBER(2)       NOT NULL, 
MATCHID     NUMBER(3)       NOT NULL, 
P1REGID     NUMBER(3)       NOT NULL, 
P2REGID     NUMBER(3)       NOT NULL, 
ROUNDS      NUMBER(1)       NOT NULL, 
CONSTRAINT PK_MATCH PRIMARY KEY (TID, MATCHID), 
CONSTRAINT FK_MTID FOREIGN KEY (TID) REFERENCES TOURNAMENT(TID), 
CONSTRAINT FK_MREGID1 FOREIGN KEY (P1REGID) REFERENCES REGISTRATION(REGID), 
CONSTRAINT FK_PREGID2 FOREIGN KEY (P2REGID) REFERENCES REGISTRATION(REGID))
;

INSERT INTO MATCH VALUES (1, 101, 3, 7, 2) 

;

INSERT INTO MATCH VALUES (1, 102, 6, 4, 3) 

;

INSERT INTO MATCH VALUES (1, 103, 2, 1, 2) 

;

INSERT INTO MATCH VALUES (1, 104, 8, 5, 2) 

;

INSERT INTO MATCH VALUES (1, 201, 7, 6, 2) 

;

INSERT INTO MATCH VALUES (1, 202, 2, 5, 2) 

;

INSERT INTO MATCH VALUES (1, 301, 7, 2, 2) 

;

INSERT INTO MATCH VALUES (2, 101, 16, 12, 0) 

;

INSERT INTO MATCH VALUES (2, 102, 15, 13, 0) 

;

INSERT INTO MATCH VALUES (2, 103, 14, 9, 0) 

;

INSERT INTO MATCH VALUES (2, 104, 10, 11, 0) 

;

COMMIT



CREATE TABLE SSET ( 
TID     NUMBER(2)       NOT NULL, 
MATCHID NUMBER(3)       NOT NULL, 
SETNUM  NUMBER(1)       NOT NULL, 
P1GAMES NUMBER(1)       NOT NULL, 
P2GAMES NUMBER(1)       NOT NULL, 
CONSTRAINT PK_SSET PRIMARY KEY (TID, MATCHID, SETNUM), 
FOREIGN KEY (TID, MATCHID) REFERENCES MATCH(TID, MATCHID))
;

