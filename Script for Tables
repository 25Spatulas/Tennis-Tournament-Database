CREATE TABLE TOURNAMENT ( 
TID         NUMBER(2)       CONSTRAINT PK_TOURNAMENT PRIMARY KEY, 
NAME        VARCHAR(30)     NOT NULL, 
LOCATION    VARCHAR(30)     NOT NULL)
;

INSERT INTO TOURNAMENT VALUES (01, 'THE UNITED ROUND ROBIN', 'UNITED KINGDOM') 

;

INSERT INTO TOURNAMENT VALUES (02, 'THE AMERICAN WORLD CUP', 'UNITED STATES OF AMERICA') 

;

COMMIT



CREATE TABLE PLAYINGIN ( 
TID         NUMBER(2)       NOT NULL, 
REGID       NUMBER(3)       NOT NULL, 
PLAYERID    NUMBER(4)       NOT NULL, 
SEED        NUMBER(1)       NOT NULL, 
CONSTRAINT PK_PLAYINGIN PRIMARY KEY (TID, REGID), 
CONSTRAINT FK_PTID FOREIGN KEY (TID) REFERENCES TOURNAMENT(TID), 
CONSTRAINT FK_PREGID FOREIGN KEY (REGID) REFERENCES REGISTRATION(REGID), 
CONSTRAINT FK_PPLAYERID FOREIGN KEY (PLAYERID) REFERENCES PLAYER(PLAYERID))
;

