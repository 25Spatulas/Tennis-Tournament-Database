CREATE TABLE REGISTRATION ( 
REGID       NUMBER(3)       CONSTRAINT PK_REGISTRATION PRIMARY KEY, 
PLAYERID    NUMBER(4)       CONSTRAINT NN_PLAYERID NOT NULL CONSTRAINT FK_PLAYERID REFERENCES PLAYER(PLAYERID), 
REGDATE     CHAR(10))
;

